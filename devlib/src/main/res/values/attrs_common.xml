<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleLayout">
        <attr name="baseStyle">
            <enum name="normal" value="0" />
            <enum name="white" value="1" />
        </attr>
        <attr name="leftBtnIcon" format="reference" />
        <attr name="leftBtnTxt" format="string|reference" />
        <attr name="titleTxt" format="string|reference" />
        <attr name="rightBtnTxt" format="string|reference" />
        <attr name="rightBtnIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="ImageAndText">
        <attr name="leftImg" format="reference" />
        <attr name="leftTxt" format="string" />
        <attr name="rightImg" format="reference" />
    </declare-styleable>

</resources>